#include <stdint.h>
#include <winsock2.h>
#include <windows.h>
#include "flags.h"
#include "ntapi.h"

{% for flag in flags.values(): %}
static const flag_repr_t _{{ flag.name }}[] = {
    {%- for row in flag.rows: %}
    {{ "{" }}{{ row.type_ }}, {{ row.name }}, "{{ row.name }}"},
    {%- endfor %}
    {FLAGTYP_NONE, 0, NULL},
};
{% endfor %}

const flag_repr_t *g_flags[FLAGCNT] = {
    FLAG_NONE,
{%- for name in flags: %}
    _{{ name }},
{%- endfor %}
};
